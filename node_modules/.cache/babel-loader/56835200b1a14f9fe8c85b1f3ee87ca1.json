AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,GAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,YAAA,CAAA,MAAA,CAAA,IAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,CAAA,QAAA;AACA,eAJA,CAHA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA;AACA,IAAA,gBAVA,4BAUA,GAVA,EAUA;AACA,MAAA,OAAA,CAAA,GAAA,wBAAA,GAAA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,GAAA;AACA,WAAA,YAAA,CAAA,KAAA,IAAA,CAAA,OAAA,EAAA,KAAA,IAAA,CAAA,QAAA;AACA,KAdA;AAeA;AACA,IAAA,mBAhBA,+BAgBA,GAhBA,EAgBA;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,GAAA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,GAAA;AACA,WAAA,YAAA,CAAA,KAAA,IAAA,CAAA,OAAA,EAAA,KAAA,IAAA,CAAA,QAAA;AACA,KApBA;AAqBA,IAAA,YArBA,wBAqBA,IArBA,EAqBA,IArBA,EAqBA;AACA,UAAA,UAAA,sBAAA,KAAA,SAAA,CAAA;;AACA,WAAA,IAAA,CAAA,SAAA,GAAA,UAAA,CAAA,MAAA,CAAA,IAAA,IAAA,IAAA,GAAA,CAAA,CAAA,EAAA,IAAA,CAAA,CAFA,CAGA;AACA;AAzBA,GAvBA;AAkDA,EAAA,KAAA,EAAA;AACA,YAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,oBACA,KAAA,IAAA,KAAA,EADA;AAAA;AAAA;AAAA;;AAAA;AAAA,8CAEA,eAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,UAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,GAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,YAAA,CAAA,MAAA,CAAA,IAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,CAAA,QAAA;AACA,eAJA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AADA;AAlDA,CAAA","sourcesContent":["<template>\r\n     <div id=\"employeeList\">\r\n      <!-- 置顶面包屑 -->\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n            <el-breadcrumb-item>员工管理</el-breadcrumb-item>\r\n            <el-breadcrumb-item>员工列表</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n\r\n        <el-card class=\"box-card\">\r\n            <!-- 搜索栏 -->\r\n            <el-row :gutter=\"20\">\r\n                <el-col :span=\"10\">\r\n                    <el-input placeholder=\"请输入姓名\" clearable v-model=\"name\">\r\n                        <el-button slot=\"append\" icon=\"el-icon-search\"></el-button>\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :span=\"5\"><el-button type=\"info\" @click=\"searchSatff\">搜索</el-button></el-col>\r\n            </el-row>\r\n            <!-- 用户列表 -->\r\n            <el-table :data=\"page.nowrecord\" stripe border  style=\"width: 100%\" height=\"500px\">\r\n                <el-table-column prop=\"employeeName\" label=\"姓名\"></el-table-column>\r\n                <el-table-column prop=\"sex\" label=\"性别\"></el-table-column>\r\n                <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n                <el-table-column prop=\"telPhone\" label=\"电话\"></el-table-column>\r\n                <el-table-column prop=\"salary\" label=\"工资\"></el-table-column>\r\n                <el-table-column label=\"操作\" width=\"240px\">\r\n                    <template slot-scope=\"scope\">\r\n                      <el-tooltip class=\"item\" effect=\"dark\" content=\"查看员工信息\" placement=\"top\">\r\n                        <el-button type=\"primary\" icon=\"el-icon-user\"></el-button>\r\n                      </el-tooltip>\r\n                      <el-tooltip class=\"item\" effect=\"dark\" content=\"调离申请\" placement=\"top\">\r\n                        <el-button type=\"danger\" icon=\"el-icon-message\"></el-button>\r\n                      </el-tooltip>\r\n                      <el-tooltip class=\"item\" effect=\"dark\" content=\"开除申请\" placement=\"top\">\r\n                        <el-button type=\"danger\" icon=\"el-icon-delete\"></el-button>\r\n                      </el-tooltip> \r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n            <!-- 分页 -->\r\n            <el-pagination @size-change=\"handleSizeChange\"\r\n                            @current-change=\"handleCurrentChange\"\r\n                            :page-sizes=\"[5, 10, 15, 20]\"\r\n                            :page-size=\"5\"\r\n                            layout=\"total, sizes, prev, pager, next, jumper\"\r\n                            :total=\"page.totalrecord\"\r\n                            align=\"center\">\r\n            </el-pagination>\r\n        </el-card>\r\n     </div>\r\n</template>\r\n\r\n<script type=\"text/javascript\">\r\nimport {GetemployeeList,SearchStaff} from './network/employeeList'\r\nexport default {\r\n  name: \"employeeList\",\r\n  data() {\r\n    return {\r\n        tableData: [],\r\n        name:\"\",\r\n        page:{\r\n          nowrecord:[],\r\n          pagesize:5,\r\n          pagenow:1,\r\n          totalrecord:0\r\n        }\r\n    }\r\n  },\r\n  components: {\r\n  },\r\n  created: async function(){\r\n    await GetemployeeList(this.$store.state.employeeId).then(res =>{\r\n      this.tableData = res\r\n      this.page.totalrecord = res.length\r\n      this.changerecord(this.page.pagenow,this.page.pagesize)\r\n    })\r\n  },\r\n  methods:{\r\n      async searchSatff(){\r\n        if(this.name === \"\") return\r\n          await SearchStaff(this.name).then(res =>{\r\n            this.tableData = res\r\n            this.page.totalrecord = res.length\r\n            this.changerecord(this.page.pagenow,this.page.pagesize)\r\n          })\r\n      },\r\n      //根据修改的size改变表格\r\n      handleSizeChange(val) {\r\n        console.log(`每页 ${val} 条`);\r\n        this.page.pagesize = val;\r\n        this.changerecord(this.page.pagenow,this.page.pagesize)\r\n      },\r\n      //根据修改的pagenow改变表格\r\n      handleCurrentChange(val) {\r\n        console.log(`当前页: ${val}`);\r\n        this.page.pagenow = val;\r\n        this.changerecord(this.page.pagenow,this.page.pagesize)\r\n      },\r\n      changerecord(page,size){\r\n        let newrecords = [...this.tableData]\r\n        this.page.nowrecord = newrecords.splice(size*(page-1),size)\r\n        // console.log(newrecords)\r\n      },\r\n  },\r\n  watch:{\r\n    'name':async function(){\r\n      if(this.name === \"\"){\r\n          await GetemployeeList(this.$store.state.employeeId).then(res =>{\r\n            this.tableData = res\r\n            this.page.totalrecord = res.length\r\n            this.changerecord(this.page.pagenow,this.page.pagesize)\r\n          })\r\n      }\r\n    } \r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"],"sourceRoot":"src/components"}]}