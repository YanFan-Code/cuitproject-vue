cAAA,OAAA,CAAA,SAAA,CAAA;AACA,gBAAA,KAAA,EAAA;AACA,kBAAA,IAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EAAA,QAFA;AAGA,kBAAA,IAAA,EAAA,QAHA;AAIA,kBAAA,KAAA,EAAA;AAJA,iBADA;AAOA,gBAAA,OAAA,EAAA,EAPA;AAQA,gBAAA,MAAA,EAAA;AACA,kBAAA,KAAA,EAAA,IADA;AAEA,kBAAA,IAAA,EAAA,CAAA,IAAA,EAAA,IAAA,EAAA,OAAA;AAFA,iBARA;AAYA;AACA,gBAAA,KAAA,EAAA;AAAA,kBAAA,IAAA,EAAA,UAAA;AAAA,kBAAA,IAAA,EAAA;AAAA,iBAbA;AAcA;AACA,gBAAA,KAAA,EAAA,EAfA;AAgBA;AACA,gBAAA,MAAA,EAAA,CACA;AAAA,kBAAA,IAAA,EAAA,IAAA;AAAA,0BAAA,KAAA;AAAA,0BAAA,IAAA,CAAA,CAAA;AAAA,iBADA,EAEA;AAAA,kBAAA,IAAA,EAAA,IAAA;AAAA,0BAAA,KAAA;AAAA,0BAAA,IAAA,CAAA,CAAA;AAAA,iBAFA,EAGA;AAAA,kBAAA,IAAA,EAAA,OAAA;AAAA,0BAAA,KAAA;AAAA,0BAAA,IAAA,CAAA,CAAA;AAAA,iBAHA;AAjBA,eAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAdA;AAkDA,EAAA,UAAA,EAAA;AAlDA,CAAA","sourcesContent":["<template>\r\n  <div id=\"\">\r\n    <!-- 置顶面包屑 -->\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n            <el-breadcrumb-item :to=\"{ path: '/loginin' }\">首页</el-breadcrumb-item>\r\n            <el-breadcrumb-item>业绩报表</el-breadcrumb-item>\r\n            <el-breadcrumb-item>年度报表</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      <div id=\"myChart\" :style=\"{width: '100%', height: '500px'}\"></div>\r\n      <div style=\"width:150px; margin-left:40%\"><span style=\"margin-left:30px\">当前季度评分</span><el-rate\r\n    v-model=\"value\"\r\n    disabled\r\n    :colors=\"colors\"\r\n    show-score\r\n    disabled-void-color=\"gray\"\r\n    score-template=\"{value}\">\r\n  </el-rate></div>\r\n  <div class=\"comment\" v-text=\"comment\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script type=\"text/javascript\">\r\nimport {getdatatableforyear} from './network/yearlyStatement'\r\nexport default {\r\n  name: \"\",\r\n  data() {\r\n    return {\r\n      value:5,\r\n      colors: ['#99A9BF', '#F7BA2A', '#FF9900'],\r\n      data:[],\r\n      value:0,\r\n      comment:''\r\n    }\r\n  },\r\n  mounted(){\r\n    this.drawLine();\r\n  },\r\n  methods: {\r\n    async drawLine(){\r\n        // 基于准备好的dom，初始化echarts实例\r\n        let myChart = this.$echarts.init(document.getElementById('myChart'))\r\n        var  xAxisData =['1月份','2月份','3月份','4月份','5月份','6月份','7月份','8月份','9月份','10月份','11月份','12月份',]\r\n        //后期这里是动态从数据库获取的json数据\r\n        var res = await getdatatableforyear(this.$store.state.employeeId)\r\n        var data = res.arr\r\n        this.value = res.sorce\r\n        this.comment = res.assess\r\n        // 绘制图表\r\n        myChart.setOption({\r\n            title:{\r\n                text:'当前年度报表',\r\n                subtext:'员工管理系统',\r\n                left:'center',\r\n                align:'right'\r\n            },\r\n            tooltip: {},\r\n            legend:{\r\n                right:'5%',\r\n                data: ['出差', '缺勤', '任务完成数']\r\n            },\r\n            // 声明一个 X 轴，类目轴（category）。默认情况下，类目轴对应到 dataset 第一列。\r\n            xAxis: {type: 'category',data:xAxisData},\r\n            // 声明一个 Y 轴，数值轴。\r\n            yAxis: {},\r\n            // 声明多个 bar 系列，默认情况下，每个系列会自动对应到 dataset 的每一列。\r\n            series: [\r\n                {name:'出差','type': 'bar','data':data[0]},\r\n                {name:'缺勤','type': 'bar','data':data[1]},\r\n                {name:'任务完成数','type': 'bar','data':data[2]},\r\n            ]\r\n                });\r\n            }\r\n  },\r\n  components: {\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.comment{\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  margin-left: 20%\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}